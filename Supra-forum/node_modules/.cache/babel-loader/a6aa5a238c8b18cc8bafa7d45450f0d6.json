div>\r\n</template>\r\n\r\n<style lang=\"less\" scoped>\r\n.topics_content {\r\n  width: 100%;\r\n  .topics_content_ul {\r\n    padding: 0;\r\n    list-style: none;\r\n    width: 100%;\r\n    position: relative;\r\n    li {\r\n      margin-top: 5px;\r\n      width: 100%;\r\n      height: 80px;\r\n      .topic_left {\r\n        float: left;\r\n        width: 20%;\r\n        height: 100%;\r\n        position: relative;\r\n        img {\r\n          width: 80%;\r\n          height: 80%;\r\n          border-radius: 50%;\r\n          position: absolute;\r\n          left: 50%;\r\n          top: 50%;\r\n          transform: translate(-50%, -50%);\r\n        }\r\n      }\r\n      .topic_right {\r\n        float: left;\r\n        width: 80%;\r\n        height: 100%;\r\n        .topic_title {\r\n          width: 100%;\r\n          overflow: hidden;\r\n          text-overflow: ellipsis;\r\n          white-space: nowrap;\r\n          height: 40px;\r\n          line-height: 40px;\r\n          font-size: 18px;\r\n        }\r\n        .topic_poster_nickname {\r\n          float: left;\r\n          width: 100%;\r\n          height: 20px;\r\n          line-height: 20px;\r\n          color: #333;\r\n          font-size: 14px;\r\n          overflow: hidden;\r\n          .mui-icon-closeempty {\r\n            float: right;\r\n          }\r\n        }\r\n        .topic_createdtime {\r\n          width: 100%;\r\n          line-height: 20px;\r\n          font-size: 10px;\r\n          color: gray;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n.mui-scroll_content {\r\n  top: 40px;\r\n  bottom: 54px;\r\n}\r\n</style>\r\n\r\n<script>\r\nimport { Toast } from \"mint-ui\";\r\nimport myaxios, {\r\n  getsessionURL,\r\n  getMyCollection,\r\n  deleteCollectionURL\r\n} from \"@/tools/myaxios.js\";\r\nimport mui from \"@/../lib/mui/js/mui.js\";\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      topics: \"\",\r\n      user: \"\",\r\n      collection: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    async deleteCollection(_id) {\r\n      var obj = {\r\n        _id: _id\r\n      };\r\n      let result = await myaxios(\"POST\", deleteCollectionURL, obj);\r\n      if (result.data.code == 101) {\r\n        Toast(result.data.message);\r\n        var obj = {\r\n          userID: this.user._id\r\n        };\r\n        let result2 = await myaxios(\"POST\", getMyCollection, obj);\r\n        if (result2.data.code == 101) {\r\n          this.collection = result2.data.collection;\r\n        }\r\n      }\r\n    }\r\n  },\r\n  async created() {\r\n    let result1 = await myaxios(\"GET\", getsessionURL);\r\n    if (result1.data.user) {\r\n      this.user = result1.data.user;\r\n      var obj = {\r\n        userID: this.user._id\r\n      };\r\n      let result2 = await myaxios(\"POST\", getMyCollection, obj);\r\n      if (result2.data.code == 101) {\r\n        this.collection = result2.data.collection;\r\n      }\r\n    } else {\r\n      this.$router.push(\"/my\");\r\n    }\r\n  },\r\n  mounted() {\r\n    var header_title = document.getElementsByClassName(\"mint-header-title\")[0];\r\n    header_title.innerText = \"我的收藏\"\r\n    mui(\".mui-scroll-wrapper\").scroll({\r\n      deceleration: 0.0005 //flick 减速系数，系数越大，滚动速度越慢，滚动距离越小，默认值0.0006\r\n    });\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views/my_components"}]}